{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "error", // comillas
    "no-console": 0, // habilita console.log
    "semi": ["error", "never"], // nunca ;
    "object-curly-newline": "off",
    "no-underscore-dangle": 0,
    "comma-dangle": 0
  }
}
